#include <iostream>
#include <iomanip>
using namespace std;
using ll = long long;
long long gcd (long long a, long long b){
	if (b==0) return a;
	return gcd(b,a%b);
}
int main(){
	int t;
	cin >> t;
	while(t--){
		ll a,b,c;
		cin >> a >> b >> c;
		for (ll i=0;i<gcd(c,b);i++) cout << a;
		cout << "\n";
	}
}
