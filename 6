#include <iostream>
using namespace std;
int check (int n){
	if (n<2) return 0;
	if (n==2) return 1;
	for (int i=2;i*i<=n;i++){
		if (n%i==0) return 0;
	}
	return 1;
}
int main(){
	int t;
	cin >> t;
	while (t--){
		int cnt=0;
		int n; cin >> n;
		if (n%2!=0){
			int m = n - 2;
			if (check(m)==1){
				cnt++;
				cout << "2 " << m;
			} 
		}
		else{
			for (int i=2;i<=n/2;i++){
				if (check(i)==1){
					int m=n-i;
					if (check(m)==1){
						cnt++;
						cout << i << " " << m;
						break;
					}
				}
			}
		}
		if (cnt==0) cout << "-1";
		cout << "\n";
	}
}
