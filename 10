#include <iostream>
#include <iomanip>
#include <string>
using namespace std;
int main(){
    int t;
    cin >> t;
    while (t--){
    	int n;
    	cin >> n;
    	long long k;
    	cin >> k;
    	long long sum = 0;
    	for (int i=1;i<=n;i++){
    		sum = sum + i%k;
		}
		if (sum==k) cout << "1";
		else cout << "0";
		cout << "\n";
	}
}
