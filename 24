#include <iostream>
#include <iomanip>
#include <string>
#include <cmath>
#include <algorithm>
int main(){
    int t;
    cin >> t;
    while (t--){
    	int n;
    	cin >> n;
    	int a[n+1];
    	for (int i=0;i<n;i++) cin >> a[i];
    	sort(a, a + n, greater<int>());
    	int x = minn (a,n);
    	cout << x << "\n";
	}
}
